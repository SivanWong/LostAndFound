/* pages/contact/contact.wxss */
page{
  width: 100%;
  height: 100%;
  background: rgb(232,230,231)  ASDF; 
}
.ourlogo{
  width: 100%;
  display: flex;
  justify-content: center;
  margin: 45rpx 0;
}
.ourlogo image{
  width:250rpx;
  height: 250rpx;
  border-radius: 50%;
  
}
.cells{
  position: relative;
  padding:15px 15px;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 17px;
  background: #fff;
   margin-bottom: 10rpx; 
  color:#333;
 
}
.cells:after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1rpx solid #D9D9D9;
  color: #D9D9D9;
}
.cells view{
  color: black;
  font-size: 18px;
}